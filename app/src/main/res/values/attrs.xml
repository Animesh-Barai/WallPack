<resources>
    <declare-styleable name="CustomTheme">
        <attr name="backdropDrawable" format="reference"/>
        <attr name="dividerColor" format="color|reference"/>
        <attr name="themeName" format="string"/>
        <attr name="themeDescription" format="string"/>
        <attr name="tabLayoutStyle" format="reference"/>
    </declare-styleable>



    <!-- circleImageView -->
    <declare-styleable name="CircleImageView">
        <attr name="civ_border_width" format="dimension" />
        <attr name="civ_border_color" format="color" />
        <attr name="civ_border_overlay" format="boolean" />
        <attr name="civ_fill_color" format="color" />
    </declare-styleable>
</resources>